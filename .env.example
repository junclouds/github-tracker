SMTP_SERVER=smtp.gmail.com  # 或其他邮件服务器地址
SMTP_PORT=587              # 邮件服务器端口
SMTP_USERNAME=your_email@gmail.com  # 您的邮箱地址
SMTP_PASSWORD=your_app_password     # 您的邮箱应用专用密码
